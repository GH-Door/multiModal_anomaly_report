# ── 이 파일을 .env로 복사 후 값을 채워주세요 ────────────────────────────────────
# cp .env.example .env

# ── PostgreSQL ──────────────────────────────────────────────────────────────
POSTGRES_PASSWORD=your_secure_password_here

# ── LLM API 키 (사용하는 서비스만 입력) ────────────────────────────────────────
OPENAI_API_KEY=YOUR_API_KEY
ANTHROPIC_API_KEY=YOUR_API_KEY
GOOGLE_API_KEY=YOUR_API_KEY
HF_TOKEN=YOUR_API_KEY
WANDB_API_KEY=YOUR_API_KEY

# ── LLM 모델 설정 ─────────────────────────────────────────────────────────────
# 선택: gpt-4o | claude | gemini | gemini-2.5-flash | internvl | qwen | llava | gemma3
LLM_MODEL=gemini-2.5-flash

# ── RAG 설정 ──────────────────────────────────────────────────────────────────
RAG_ENABLED=true
DOMAIN_RAG_ENABLED=true

# ── 자동 수신 설정 ─────────────────────────────────────────────────────────────
INCOMING_WATCH_ENABLED=true
